<BootUp>:
    name: 'boot_up'
    FloatLayout:
        Image:
            source: 'Kuva_logo_red.png'
            size_hint: 0.5, 0.5
            pos_hint: {'center_x': 0.5, 'center_y': 0.6}

        Button:
            text: 'Click Here to Begin'
            bold: 'True'
            markup: 'True'
            background_normal: 'normal_red.png'
            background_down: 'down_red.png'
            border: 0, 0, 0, 0
            size: 200, 100
            size_hint: None, None
            pos_hint: {'center_x': 0.5, 'center_y': 0.3}
#            background_normal: ''
#            background_color: 0.839, 0.188, 0.192, 1
            on_release: app.root.current = 'sign_up'

<SignUp>:
    name: 'sign_up'
    FloatLayout:

        Label:
            text: 'Create an Account'
            color: 1, 1, 1, 1
            font_size: 30
            pos_hint: {'center_x': 0.5, 'center_y': 0.85}

        TextInput:
            text: 'Email'
            size_hint: 0.3, 0.08
            pos_hint: {'center_x': 0.5, 'center_y': 0.7}

        TextInput:
            text: 'Username'
            size_hint: 0.3, 0.08
            pos_hint: {'center_x': 0.5, 'center_y': 0.6}

        TextInput:
            text: 'Password'
            size_hint: 0.3, 0.08
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}

        Button:
            text: 'Sign Up'
            bold: 'True'
            markup: 'True'
            background_normal: 'normal_red.png'
            background_down: 'down_red.png'
            border: 0, 0, 0, 0
            size: 200, 100
            size_hint: None, None
            pos_hint: {'center_x': 0.5, 'center_y': 0.35}
            on_release: app.root.current = 'movie_input'

        Button:
            text: 'Already have an account? Sign in here.'
            bold: 'True'
            markup: 'True'
            background_normal: 'normal_red.png'
            background_down: 'down_red.png'
            border: 0, 0, 0, 0
            size: 500, 100
            size_hint: None, None
            pos_hint: {'center_x': 0.5, 'center_y': 0.13}
            on_release: app.root.current = 'sign_in'

<SignIn>:
    name: 'sign_in'
    FloatLayout:
        Label:
            text: 'Sign In'
            color: 1, 1, 1, 1
            font_size: 30
            pos_hint: {'center_x': 0.5, 'center_y': 0.85}

        TextInput:
            text: 'Username'
            size_hint: 0.3, 0.08
            pos_hint: {'center_x': 0.5, 'center_y': 0.7}

        TextInput:
            text: 'Password'
            size_hint: 0.3, 0.08
            pos_hint: {'center_x': 0.5, 'center_y': 0.6}

        Button:
            text: 'Sign In'
            bold: 'True'
            markup: 'True'
            background_normal: 'normal_red.png'
            background_down: 'down_red.png'
            border: 0, 0, 0, 0
            size: 200, 100
            size_hint: None, None
            pos_hint: {'center_x': 0.5, 'center_y': 0.45}
            on_release: app.root.current = 'movie_input'

